{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest"
    ],
    "jsRules": {},
    "rules": {
        "max-line-length": {
            "severity": "off"
        },
        "no-console": false,
        "semicolon": true,
        "no-namespace": false,
        "arrow-parens": false,
        "max-classes-per-file": false,
        "ordered-imports": false,
        "no-string-literal": false,
        "variable-name": false,
        "no-unused-expression": true,
        "prefer-readonly": true,
        "interface-name": true,
        "no-trailing-whitespace": true
    },
    "rulesDirectory": []
}