{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest"
    ],
    "jsRules": {},
    "rules": {
        "max-line-length": {
            "severity": "off"
        },
        "no-console": false,
        "semicolon": true,
        "no-namespace": false,
        "arrow-parens": false,
        "max-classes-per-file": false
    },
    "rulesDirectory": []
}